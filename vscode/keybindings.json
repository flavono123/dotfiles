// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "cmd+g cmd+h",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "git chore\n"
    }
  },
  {
    "key": "cmd+g cmd+f",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "git push -f\n"
    }
  },
  {
    "key": "cmd+g cmd+s",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "git status\n"
    }
  },
  {
    "key": "cmd+g cmd+b",
    "command": "git.branch"
  },
  {
    "key": "cmd+g cmd+p",
    "command": "git.push"
  },
  {
    "key": "shift+alt+f",
    "command": "-editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
    "key": "cmd+g cmd+u",
    "command": "git.unstageAll"
  },
  {
    "key": "cmd+g cmd+a",
    "command": "git.commitAllAmend"
  },
  {
    "key": "cmd+g cmd+o",
    "command": "git.checkout"
  },
  {
    "key": "cmd+g cmd+l",
    "command": "git.pull"
  },
  {
    "key": "ctrl+shift+c",
    "command": "remote-containers.openFolder"
  },
  {
    "key": "cmd+g cmd+r",
    "command": "pr.create"
  },
  {
    "key": "cmd+g cmd+c",
    "command": "git.commitAllSigned"
  },
  {
    "key": "ctrl+d",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+s",
    "command": "-workbench.action.files.saveAs"
  },
  {
    "key": "shift+cmd+s",
    "command": "editor.action.sortLinesAscending"
  },
  {
    "key": "ctrl+shift+l",
    "command": "editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+l",
    "command": "-editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+l",
    "command": "dendron.lookupNote",
    "when": "dendron:pluginActive"
  },
  {
    "key": "cmd+l",
    "command": "-dendron.lookupNote",
    "when": "dendron:pluginActive"
  },
  {
    "key": "shift+cmd+v",
    "command": "markdown.showPreviewToSide",
    "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  },
  {
    "key": "cmd+k v",
    "command": "-markdown.showPreviewToSide",
    "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  },
  {
    "key": "shift+cmd+v",
    "command": "-markdown.showPreview",
    "when": "!notebookEditorFocused && editorLangId == 'markdown'"
  },
  {
    "key": "shift+cmd+d",
    "command": "editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+l",
    "command": "dendron.createNote"
  },
  {
    "key": "ctrl+n ctrl+f",
    "command": "explorer.newFolder"
  },
  {
    "key": "shift+cmd+up",
    "command": "-dendron.goUpHierarchy",
    "when": "dendron:pluginActive && editorFocus"
},
{
    "key": "ctrl+n",
    "command": "composer.createNewChat",
    "when": "focusedView == 'workbench.panel.aichat.view'"
},
{
    "key": "cmd+n",
    "command": "-composer.createNewChat",
    "when": "focusedView == 'workbench.panel.aichat.view'"
},
{
    "key": "shift+cmd+down",
    "command": "-dendron.goDownHierarchy",
    "when": "dendron:pluginActive && editorFocus"
},
{
    "key": "ctrl+cmd+c",
    "command": "issue.copyGithubPermalink",
    "when": "!isInEmbeddedEditor"
}
]
